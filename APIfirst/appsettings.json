{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ListingDb": "Host=127.0.0.1;Port=5432;Username=admin;Password=admin;Database=  PGONE;Pooling=true;Maximum Pool Size=100"
  },
  "Redis": {
    "host": "127.0.0.1",
    "port": 6379,
    "maxWritePoolSize": 60,
    "maxReadPoolSize": 60,
    "autoStart": true,
    "localCacheTime": 36000,
    "db": 0

  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug", //最小日志记录级别
      "Override": { //系统日志最小记录级别
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }, //输出到控制台
      {
        "Name": "Async", //Serilog.Sinks.Async
        "Args": {
          "configure": [
            {
              "Name": "File", //输出文件
              "Args": {
                "path": "logs/log.txt",
                "outputTemplate": "{NewLine}Date:{Timestamp:yyyy-MM-dd HH:mm:ss.fff}{NewLine}LogLevel:{Level}{NewLine}Message:{Message}{NewLine}Exception:{Exception}{NewLine}---------------------------------------------------------------------------------------------------------",
                "rollingInterval": "3" //按天记录
              }
            }
          ]
        }
      }
    ]
  }
}
